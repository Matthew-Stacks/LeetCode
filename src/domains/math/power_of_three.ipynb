   "source": [
    "from math import floor, log\n",
    "\n",
    "class Solution:\n",
    "    def isPowerOfThree(self, n: int) -> bool:\n",
    "        MAX_INT = 2_147_483_647\n",
    "        significand = floor(log(MAX_INT, 3))\n",
    "        valid_n = [int(3**idx) for idx in range(0,significand+1)]\n",
    "        return n in valid_n"
   ]
  }
